# Tests lock --last for a project with no backups.
# Uses short option names.
#
# Uses m4.
#

m4_include(common.m4)

application:copyback-locking
name:lock-last-empty-explicit-2
home:..

setup.install:data/empty

test.run.cmdline/prepend-home:M4_SRC_BIN/copyback --prfn lock -le empty

check.exitcode:M4_EXIT_ERROR

check.stdout.empty

# Check for the right number of files.
check.run.cmdline:test $(ls empty | wc -l) == '2'
