# Tests that the project name is set correctly: tests a basename name.
#

m4_include(common.m4)

application:copyback
name:prj-name/tail/basename
home:..

test.run.cmdline/prepend-home:M4_SRC_BIN/copyback --basename --debug -- -c print-prjname-tail ${SIMPTEST_HOME}/data/prjname-test-file

# "Standard" general checks.
check.exitcode:0
check.stderr.empty

check.stdout.file.eq:output/basename.stdout.txt
